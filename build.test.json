{
  "compilerOptions": {
    "module": "es2022",
    "target": "es2022",
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "baseUrl": "./",
    "paths": {
      "utilities": ["./src/utilities/Utilities.js"],
      "types": ["./src/Generic.types.js"],
      "object": ["./src/Object.js"],
      "tuple": ["./src/Tuple.js"]
    }
  },
  "include": ["./src/**/*", "./test/**/*"],
  "typedocOptions": {
    "excludePrivate": true,
    "excludeProtected": true,
    "disableSources": true,
    "categorizeByGroup": false,
    "sort": ["static-first", "visibility"],
    "name": "ES6 Module",
    "hideGenerator": true
  }
}
